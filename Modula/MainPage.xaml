<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="Modula.MainPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:components="clr-namespace:Modula.Components"
    xmlns:local="clr-namespace:Modula">

    <BlazorWebView
        x:Name="blazorWebView"
        HostPage="wwwroot/index.html"
        StartPath="/login">
        <BlazorWebView.RootComponents>
            <RootComponent ComponentType="{x:Type components:Routes}" Selector="#app" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
